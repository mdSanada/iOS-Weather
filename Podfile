# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'iOS-Weather' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOS-Weather
  
  # UI
  pod 'SnapKit'

  # Networking
  pod 'Resolver'
  pod 'KeychainSwift'
  pod 'Moya/RxSwift'
  
  # Animations
  pod 'Hero'
  
  # Reactive
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'RxCoreLocation'
  pod 'RxSwiftExt'
  pod 'RxGesture'
  pod 'RxOptional'
  pod 'CocoaLumberjack/Swift'
end
